{
    "extends": "airbnb",
    "rules": { 
        "camelcase": [0],
        "class-methods-use-this": [0],
        "no-console": [0],
        "no-mixed-operators": [0],
        "no-plusplus": [0], 
        "no-underscore-dangle": [0],
        "no-undef": [0],
        "prefer-const": [0],
        "react/jsx-filename-extension": [0]
    }
}
